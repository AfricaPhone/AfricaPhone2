rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {

    
    match /{allPaths=**} {
      allow read: if true;
    }

    
    match /product-images/{allPaths=**} {
      allow write: if request.auth != null;
    }

    
    match /candidates_photos/{contestId}/{userId}/{fileName} {
      allow write: if request.auth != null && request.auth.uid == userId;
    }
  }
}