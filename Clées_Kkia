# Secrets KKiaPay à enregistrer dans Firebase Functions
# (supprimer les deux-points qui étaient placés devant les valeurs)
firebase functions:secrets:set KKIA_PUBLIC_KEY c2811a3222019d25a6ec80e33c147ae761093089
firebase functions:secrets:set KKIA_PRIVATE_KEY pk_019e9ab797774d15fd9112f47a093cdabe7769fc6c822dd69e3cf7d786b5a34e
firebase functions:secrets:set KKIA_SECRET_KEY sk_6c325db81b9e5ca0d9a2b51081d367670ee9fc5699691d0247adabcb8b171670
firebase functions:secrets:set KKIA_WEBHOOK_SECRET 4DM/q1AjaRe68DLEskbvmFuVbrKyj/6hNbX/usRTHG0=
# Sandbox: true pour l'environnement de test, false en production
firebase functions:secrets:set KKIA_SANDBOX true

